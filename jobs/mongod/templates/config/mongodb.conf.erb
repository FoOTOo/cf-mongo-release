systemLog:
   destination: file
   path: "<%= p("mongodb.MONGODB_SYSTEMLOG") %>"
   quiet: false
   logAppend: true

storage:
  dbPath: "<%= p("mongodb.MONGODB_STORAGE_DBPATH") %>"
  directoryPerDB: true
  smallFiles: true
  journal:
    enabled: true

security:
  authorization: enabled

net:
  bindIp: <%= p("mongodb.MONGODB_BINDIP") %>
  port: <%= p("mongodb.MONGODB_PORT") %>
