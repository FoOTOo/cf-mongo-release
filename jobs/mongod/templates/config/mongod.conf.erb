systemLog:
   destination: file
   path: "<%= p("mongod.MONGODB_SYSTEMLOG") %>"
   quiet: false
   logAppend: true

storage:
  dbPath: "<%= p("mongod.MONGODB_STORAGE_DBPATH") %>"
  directoryPerDB: true
  smallFiles: true
  journal:
    enabled: true

security:
  authorization: enabled
  keyFile: /srv/mongodb/keyfile

net:
  bindIp: <%= p("mongod.MONGODB_BINDIP") %>
  port: <%= p("mongod.MONGODB_PORT") %>
